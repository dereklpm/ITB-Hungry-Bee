{"ast":null,"code":"export default {\n  name: \"CartItemCounter\",\n  props: {\n    quantity: {\n      type: Number,\n      default: 1\n    }\n  },\n  computed: {\n    localQuantity: {\n      get() {\n        return this.quantity;\n      },\n      set(value) {\n        this.$emit(\"input\", value);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAaA;EACAA;EACAC;IACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACAC;QACA;MACA;MACAC;QACA;MACA;IACA;EACA;AACA","names":["name","props","quantity","type","default","computed","localQuantity","get","set"],"sourceRoot":"src/components/Cards","sources":["CartItemCounter.vue"],"sourcesContent":["<template>\n  <div class=\"cart-counter\">\n    <button class=\"decrement\" @click=\"quantity--\">\n      <i class=\"fas fa-minus\"></i>\n    </button>\n    <input type=\"number\" v-model=\"quantity\"/>\n    <button class=\"increment\" @click=\"quantity++\">\n      <i class=\"fas fa-plus\"></i>\n    </button>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"CartItemCounter\",\n  props: {\n    quantity: {\n      type: Number,\n      default: 1\n    }\n  },\n  computed: {\n    localQuantity: {\n      get() {\n        return this.quantity;\n      },\n      set(value) {\n        this.$emit(\"input\", value);\n      }\n    }\n  }\n};\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}