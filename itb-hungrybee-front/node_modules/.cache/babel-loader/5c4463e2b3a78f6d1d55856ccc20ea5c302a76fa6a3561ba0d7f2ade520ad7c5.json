{"ast":null,"code":"import { mapState, mapMutations } from \"vuex\";\nexport default {\n  name: \"OrdersPage\",\n  computed: {\n    ...mapState(\"cart\", [\"cartCount\", \"cart\"]),\n    ...mapState(\"user\", [\"address\", \"paymentMethods\"])\n  },\n  data() {\n    return {\n      editAddress: false,\n      newAddress: \"\",\n      subTotal: 0,\n      deliveryFee: 0,\n      serviceFee: 0,\n      totalPrice: 0\n    };\n  },\n  methods: {\n    ...mapMutations(\"user\", [\"updateAddress\", \"addPaymentMethod\"]),\n    updateAddress() {\n      this.updateAddress(this.newAddress);\n      this.newAddress = \"\";\n      this.editAddress = false;\n    },\n    updateTotal() {\n      this.subTotal = this.cart.reduce((acc, item) => acc + item.price * item.quantity, 0);\n      this.deliveryFee = 20;\n      this.serviceFee = this.subTotal * 0.025;\n      this.totalPrice = this.subTotal + this.deliveryFee + this.serviceFee;\n    }\n  }\n};","map":{"version":3,"mappings":"AAwCA;AAEA;EACAA;EACAC;IACA;IACA;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;MACA;IACA;EACA;AACA","names":["name","computed","data","editAddress","newAddress","subTotal","deliveryFee","serviceFee","totalPrice","methods","updateAddress","updateTotal"],"sourceRoot":"src/pages","sources":["OrdersPage.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h2 class=\"orders-page__heading\">Order</h2>\n    <div class=\"orders-page__content\">\n      <div class=\"orders-page__left-column\">\n        <h3 class=\"orders-page__content-title\">Deliver Location</h3>\n        <div class=\"orders-page__content-item\">\n          <div v-if=\"!editAddress\">\n            <p>Address: {{ address }}</p>\n            <button @click=\"editAddress = true\">Edit</button>\n          </div>\n          <div v-else>\n            <label for=\"address\">Address:</label>\n            <input id=\"address\" v-model=\"newAddress\" />\n            <button @click=\"updateAddress\">Confirm</button>\n          </div>\n        </div>\n        <h3 class=\"orders-page__content-title\">Payment Method</h3>\n        <div v-for=\"(paymentMethod, index) in paymentMethods\" :key=\"index\" class=\"orders-page__content-item\">\n          <p>Card: {{ paymentMethod.cardNumber }}</p>\n          <p>Exp: {{ paymentMethod.expDate }}</p>\n        </div>\n        <button @click=\"addPaymentMethod\">Add Payment Method</button>\n      </div>\n      <div class=\"orders-page__right-column\">\n        <div class=\"orders-page__cart\">\n          <h3>Cart</h3>\n          <div v-for=\"(product, index) in $createElement\" :key=\"index\">\n            <p>{{ product.name }} x {{ product.quantity }}</p>\n            <p>{{ product.price }}</p>\n          </div>\n          <p>Total Price: {{ totalPrice }}</p>\n        </div>\n      </div>\n    </div>\n    <button class=\"orders-page__submit-button\">Submit Order</button>\n  </div>\n</template>\n\n<script>\nimport { mapState, mapMutations } from \"vuex\";\n\nexport default {\n  name: \"OrdersPage\",\n  computed: {\n    ...mapState(\"cart\", [\"cartCount\", \"cart\"]),\n    ...mapState(\"user\", [\"address\", \"paymentMethods\"]),\n  },\n  data() {\n    return {\n      editAddress: false,\n      newAddress: \"\",\n      subTotal: 0,\n      deliveryFee: 0,\n      serviceFee: 0,\n      totalPrice: 0,\n    };\n  },\n  methods: {\n    ...mapMutations(\"user\", [\"updateAddress\", \"addPaymentMethod\"]),\n    updateAddress() {\n      this.updateAddress(this.newAddress);\n      this.newAddress = \"\";\n      this.editAddress = false;\n    },\n    updateTotal() {\n      this.subTotal = this.cart.reduce((acc, item) => acc + item.price * item.quantity, 0);\n      this.deliveryFee = 20;\n      this.serviceFee = this.subTotal * 0.025;\n      this.totalPrice = this.subTotal + this.deliveryFee + this.serviceFee;\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.orders-page{\n  &__heading {\n    font-size: 28px;\n    margin-bottom: 10px;\n  }\n\n  &__content {\n    display: flex;\n    justify-content: space-between;\n  }\n\n  &__left-column {\n    flex: 0 1 65%;\n  }\n\n  &__right-column {\n    flex: 0 1 33%;\n  }\n}\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}