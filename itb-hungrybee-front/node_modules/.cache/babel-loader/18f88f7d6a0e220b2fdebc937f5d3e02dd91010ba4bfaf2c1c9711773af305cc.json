{"ast":null,"code":"export default {\n  name: \"CartItemCounter\",\n  props: {\n    quantity: {\n      type: Number,\n      default: 1\n    }\n  },\n  methods: {\n    increaseQuantity() {\n      this.quantity++;\n    },\n    decreaseQuantity() {\n      this.quantity--;\n    },\n    emitUpdate() {\n      this.$emit('update:quantity', this.quantity);\n    }\n  }\n};","map":{"version":3,"mappings":"AAaA;EACAA;EACAC;IACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","props","quantity","type","default","methods","increaseQuantity","decreaseQuantity","emitUpdate"],"sourceRoot":"src/components/Cards","sources":["CartItemCounter.vue"],"sourcesContent":["<template>\n  <div class=\"cart-counter\">\n    <button class=\"decrement\" @click=\"decreaseQuantity\" :disabled=\"quantity <= 1\">\n      <i class=\"fas fa-minus\"></i>\n    </button>\n    <input type=\"number\" v-model=\"quantity\"/>\n    <button class=\"increment\" @click=\"increaseQuantity\">\n      <i class=\"fas fa-plus\"></i>\n    </button>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"CartItemCounter\",\n  props: {\n    quantity: {\n      type: Number,\n      default: 1\n    }\n  },\n  methods:{\n    increaseQuantity(){\n      this.quantity++;\n    },\n    decreaseQuantity(){\n      this.quantity--;\n    },\n    emitUpdate(){\n      this.$emit('update:quantity', this.quantity);\n    }\n  }\n};\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}